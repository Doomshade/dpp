FUNc fibonacci_rec(pp -> num) -> pp {
    if (num < 2) {
        bye num;
    }
    bye fibonacci_rec(num - 1) + fibonacci_rec(num - 2);
}

FUNc fibonacci(pp -> num) -> pp {
    pp -> a = 0;
    pp -> b = 1;
    pp -> c = 1;
    for(i = 1 to num) {
        c = a + b;
        a = b;
        b = c;
    }
    bye c;
}

FUNc main() -> pp {
    bye fibonacci(10) + fibonacci_rec(10);
}
